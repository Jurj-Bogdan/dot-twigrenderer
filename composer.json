{
    "name": "dotkernel/dot-twigrenderer",
    "type": "library",
    "description": "DotKernel component providing twig extensions and customization",
    "license": "MIT",
    "authors": [
        {
            "name": "DotKernel Team",
            "email": "team@dotkernel.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "laminas/laminas-servicemanager": "^3.4",
        "mezzio/mezzio-twigrenderer": "^2.6.1",
        "laminas/laminas-view": "^2.11.4",
        "ocramius/proxy-manager": "^2.7",
        "laminas/laminas-authentication": "^2.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1.1",
        "squizlabs/php_codesniffer": "^3.5.4",
        "dotkernel/dot-navigation": "^2.0",
        "dotkernel/dot-flashmessenger": "^2.0",
        "laminas/laminas-form": "^2.14.5",
        "doctrine/doctrine-module": "^4.0.2",
        "laminas/laminas-dependency-plugin": "^1.0.3",
        "mezzio/mezzio-authorization": "^1.0.1"
    },
    "autoload": {
        "psr-4": {
            "Dot\\Twig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DotTest\\Twig\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev",
            "dev-develop": "2.1-dev"
        }
    }
}
