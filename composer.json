{
  "name": "dotkernel/dot-twigrenderer",
  "type": "library",
  "description": "DK useful twig extensions and ZF3 view helpers made available for twig",
  "license": "MIT",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "psr/http-message": "^1.0",
    "container-interop/container-interop": "^1.1",
    "zendframework/zend-expressive-twigrenderer": "^1.1",
    "zendframework/zend-view": "^2.8",
    "zendframework/zend-servicemanager": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3",

    "dotkernel/dot-authorization": "0.6.x-dev",
    "dotkernel/dot-authentication": "0.6.x-dev",
    "dotkernel/dot-navigation": "0.6.x-dev",
    "dotkernel/dot-flashmessenger": "0.6.x-dev",

    "zendframework/zend-form": "^2.9"
  },
  "autoload": {
    "psr-4": {
      "Dot\\Twig\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Twig\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.6-dev"
    }
  }
}
